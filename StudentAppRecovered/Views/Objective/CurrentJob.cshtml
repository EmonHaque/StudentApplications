<form id="CurrentJobForm" class="wow bounceInRight">
    <div class="card wow bounceInRight z-depth-3" style="border:1px solid #e65100">  
        <div class="card-title orange darken-3">
            <div class="header2 white-text" style="margin:0 10px"><b>Current Job</b></div>
        </div>    
        <div class="card-content">
            <div class="row">
                <div class="input-field col m12">
                    <span class="fa fa-shield orange-text text-darken-3 prefix" />
                    <input title="CurrentPosition" name="txtCurrentPosition" type="text" maxlength="50">
                    <label for="txtCurrentPosition">Position*</label>
                </div>
                <div class="input-field col m12">
                    <span class="fa fa-bank orange-text text-darken-3 prefix" />
                    <input title="CurrentOrganisation" name="txtCurrentOrganisation" type="text" maxlength="30">
                    <label for="txtCurrentOrganisation">Organisation*</label>
                </div>

                <div class="input-field col s6">
                    <span class="fa fa-calendar orange-text text-darken-3 prefix" />
                    <input title="CurrentExperienceFrom" name="txtCurrentExperienceFrom" type="text" class="datepicker">
                    <label for="txtCurrentExperienceFrom">from*</label>
                </div>

                <div class="input-field col m12">
                    <span class="fa fa-bars orange-text text-darken-3 prefix" />
                    <textarea title="CurrentResponsibilities" name="txtCurrentResponsibilities" class="materialize-textarea"></textarea>
                    <label for="txtCurrentResponsibilities">Responsibilities *</label>
                </div>
            </div>
        </div>
    </div>
</form>
